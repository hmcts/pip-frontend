  "scripts": {
    "start": "USE_PROTOTYPE=true NODE_ENV=production ./node_modules/.bin/ts-node -r tsconfig-paths/register src/main/server.ts",

    "start:dev": "export REDIS_LOCAL=true && nodemon --config nodemon.json",
    "start:dev-windows": "set REDIS_LOCAL=true && nodemon --config nodemon.json",
    "lint": "stylelint **/*.scss && eslint . --ext .js,.jsx,.ts,.tsx",

    "build": "yarn webpack --config webpack.config.js",
    "build:prod": "NODE_ENV=production yarn webpack --mode production --config webpack.config.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:routes": "jest -c jest.routes.config.js",
    "test:a11y": "jest -c jest.a11y.config.js --maxWorkers 15",
    "sonar-scan": "sonar-scanner",
    "test:smoke": "export NODE_TLS_REJECT_UNAUTHORIZED=0 && jest -c jest.smoke.config.js",
    "test:functional": "USE_PROTOTYPE=true USE_HEADLESS=true npx wdio run wdio.conf.js",
    "test:functional-dev": "npx wdio run wdio.conf.js",
    "cichecks": "yarn && yarn build && yarn lint  && yarn test && yarn test:routes &&  yarn test:a11y"
  },
