{% extends "../template.njk" %}
{% from "../macros/common-components.njk" import successPanel, backButtonHistory %}

{% block pageTitle %}
    {{ title }}
{% endblock %}
{% block beforeContent %}
    {{ super() }}
{% endblock %}
{% block content %}
    {{ successPanel(header, panelBodyMessage) }}
    <p class="govuk-body govuk-!-font-weight-bold govuk-!-margin-top-6">{{ nextMessage }}</p>
    {% if nonStrategicUpload %}
        <p class="govuk-body govuk-!-margin-0 govuk-!-font-weight-bold"><a class="govuk-link " href="manual-upload?non-strategic=true">{{ uploadFile }}</a></p>
    {% else %}
        <p class="govuk-body govuk-!-margin-0 govuk-!-font-weight-bold"><a class="govuk-link " href="manual-upload">{{ uploadFile }}</a></p>
    {% endif %}
    <p class="govuk-body govuk-!-margin-0 govuk-!-font-weight-bold"><a class="govuk-link" href="remove-list-search">{{ removeFile }}</a></p>
    <p class="govuk-body govuk-!-margin-0 govuk-!-font-weight-bold"><a class="govuk-link" href="admin-dashboard">{{ homeLinkName }}</a></p>
{% endblock %}
{% block bodyEnd %}
    {{ super() }}
    {{ backButtonHistory(cspNonce) }}
{% endblock %}
