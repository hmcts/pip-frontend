#!groovy

@Library("Infrastructure")

def type = "nodejs"
def product = "pip"
def component = "frontend"

withPipeline(type, product, component) {

  after('build') {
      echo 'after the build'
      yarnBuilder.yarn('build')
    }
  before('test') {
    echo 'before the tests'
    sh 'yarn build'
  }

    enableSlackNotifications('#pip-build-notices')
}
